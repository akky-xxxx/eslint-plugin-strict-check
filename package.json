{
  "name": "eslint-plugin-strict-check",
  "version": "0.0.23",
  "packageManager": "yarn@4.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/akky-xxxx/eslint-plugin-strict-check.git"
  },
  "homepage": "https://github.com/akky-xxxx/eslint-plugin-strict-check",
  "license": "MIT",
  "main": "out/index.js",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:1": "rm -rf ./out",
    "build:2": "swc \"./src\" -d \"./out\" --config-file .swcrc",
    "build:3": "mv ./out/src/* ./out",
    "build:4": "rm -rf ./out/src",
    "check-all": "npm-run-all fix check-code test",
    "check-code": "run-p spell-check lint type-check",
    "check-out-file": "node ./scripts/check-out-file/index.js",
    "dev": "run-s build build:watch",
    "fix": "run-p \"fix:*\" -c",
    "fix:eslint": "eslint \"./\" --fix --max-warnings 0",
    "fix:pack": "fixpack",
    "fix:prettier": "prettier \"./*.json\" \"!**/.next\" --write",
    "lint": "run-p \"lint:*\" -c",
    "lint:editorconfig": "editorconfig-checker",
    "lint:eslint": "eslint \"./\" --max-warnings 0",
    "lint:pack": "fixpack --dryRun",
    "lint:prettier": "prettier \"./*.json\" \"!**/.next\" --check",
    "prepare": "husky install",
    "spell-check": "cspell \"./{src,spec}/**/*.ts\"",
    "tagging": "node ./scripts/tagging/index.js",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "remeda": "2.2.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.3.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@typescript-eslint/rule-tester": "^7.10.0",
    "@typescript-eslint/utils": "^7.10.0",
    "chokidar": "^3.6.0",
    "cspell": "^8.8.3",
    "editorconfig-checker": "^5.1.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "fixpack": "^4.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.10.0"
  }
}
