{
  "name": "eslint-plugin-strict-check",
  "version": "0.0.18",
  "packageManager": "yarn@4.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/akky-xxxx/eslint-plugin-strict-check.git"
  },
  "homepage": "https://github.com/akky-xxxx/eslint-plugin-strict-check",
  "license": "MIT",
  "main": "out/index.js",
  "scripts": {
    "build": "swc \"./src\" -d \"./out\" --config-file .swcrc",
    "check-all": "npm-run-all fix check-code test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "run-s build build:watch",
    "fix": "run-p \"fix:*\" -c",
    "fix-ts-by-eslint": "eslint \"./{src,spec}/**/*.ts\" --fix --max-warnings 0",
    "fix-ts-by-prettier": "prettier \"./{src,spec}/**/*.ts\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-prettier fix-ts-by-eslint",
    "lint": "run-p \"lint:*\" -c",
    "lint-ts-by-eslint": "eslint \"./{src,spec}/**/*.ts\" --max-warnings 0",
    "lint-ts-by-prettier": "prettier \"./{src,spec}/**/*.ts\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:editorconfig": "editorconfig-checker",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "prepare": "husky install",
    "spell-check": "cspell \"./{src,spec}/**/*.ts\"",
    "tagging": "node ./scripts/tagging/index.js",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "remeda": "1.29.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@stylistic/eslint-plugin": "^1.4.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.38",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@typescript-eslint/rule-tester": "^6.12.0",
    "@typescript-eslint/utils": "^6.12.0",
    "chokidar": "^3.5.3",
    "cspell": "^8.0.0",
    "editorconfig-checker": "^5.1.2",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-strict-check": "^0.0.22",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^49.0.0",
    "fixpack": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "typescript": "5.3.3"
  }
}
